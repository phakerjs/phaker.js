{
  "name": "phaker.js",
  "description": "Generate massive amounts of fake contextual data",
  "version": "6.0.0",
  "contributors": [
    "Kam Lasater <kam.lasater@cyclic.sh>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/phakerjs/phaker.js.git"
  },
  "keywords": [
    "data",
    "dummy",
    "fake",
    "faker",
    "fakerjs",
    "faker.js",
    "fakr",
    "generator",
    "identity",
    "mock",
    "phaker",
    "phakerjs",
    "phaker.js",
    "phakr",
    "test"
  ],
  "scripts": {
    "browser": "./node_modules/.bin/gulp browser",
    "jsdoc": "./node_modules/.bin/gulp jsdoc",
    "readme": "./node_modules/.bin/gulp readme",
    "lint": "node_modules/.bin/jshint ./lib --config ./.jshintrc",
    "test": "node_modules/.bin/mocha test/*.*.js",
    "test:coverage": "nyc --reporter=text-lcov node_modules/.bin/mocha test/*.*.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "coveralls": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-mustache": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "jsdoc": "^3.4.0",
    "jshint": "0.9.0",
    "lodash": "^4.6.1",
    "minami": "^1.2.3",
    "mocha": "^8.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-minify": "*",
    "nyc": "^15.1.0",
    "optimist": "0.3.5",
    "sinon": "^9.0.3",
    "through2": "2.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vinyl-transform": "^1.0.0"
  },
  "license": "MIT",
  "main": "index.js"
}
